<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<component-definition>
	<identification>
		<localized-name>
			<string lang="en">Multi-Comparison Collection</string>
		</localized-name>
		<version>100</version>
		<version-string>Version 1.0.0, Pachyderm 2.0</version-string>
		<component-identifier>pachyderm.multi_comparison.collection</component-identifier>
		<conforming-identifiers>
			<identifier>pachyderm.abstract.menu</identifier>
		</conforming-identifiers>
	</identification>
	<binding-list>
		<binding type="attribute" container="array">
			 <x-auto-instantiate>YES</x-auto-instantiate>
			<binding-key>item</binding-key>
			<localized-name>
				<string lang="en">Multi-comparison Item</string>
			</localized-name>
			<content-types>
				<identifier>pachyderm.multi_comparison.item</identifier>
				<identifier>pachyderm.abstract.item</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Multi-Comparison Item</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
				<limit for="pachyderm.multi_comparison.item" strict="YES">
					<key>min-length</key>
					<integer>3</integer>
					<key>max-length</key>
					<integer>8</integer>
				</limit>
			</limits>
		</binding>
	</binding-list>
	<binding-validators>
	</binding-validators>
	<user-interface>
		<preferred-binding-ordering>
			<binding-key>item</binding-key>
		</preferred-binding-ordering>
	</user-interface>
	<environment-support>
		<environment>
			<identifier>pachyderm.flash-environment</identifier>
			<min-size>{0.0, 0.0}</min-size>
			<max-size>{0.0, 0.0}</max-size>
			<auto-layout>NO</auto-layout>
			<accepts-children>NO</accepts-children>
			<resources>
				<resource></resource>
			</resources>
		</environment>
	</environment-support>
</component-definition>