<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<component-definition>
	<identification>
		<localized-name>
			<string lang="en">Multi-Comparison Item</string>
		</localized-name>
		<version>100</version>
		<version-string>Version 1.0.0, Pachyderm 2.0</version-string>
		<component-identifier>pachyderm.multi_comparison.item</component-identifier>
		<conforming-identifiers>
			<identifier>pachyderm.abstract.item</identifier>
		</conforming-identifiers>
	</identification>
	<binding-list>
		<binding type="attribute" container="none">
			<binding-key>icon</binding-key>
			<localized-name>
				<string lang="en">Icon</string>
			</localized-name>
			<content-types>
				<identifier>public.image</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose an icon image.</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
				<limit for="public.image" strict="YES">
					<key>min-size</key>
					<size>{72, 72}</size>
					<key>max-size</key>
					<size>{72, 72}</size>
				</limit>
			</limits>
		</binding>		
		<binding type="attribute" container="none">
			<binding-key>image</binding-key>
			<localized-name>
				<string lang="en">Image</string>
			</localized-name>
			<content-types>
				<identifier>public.image</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose an image.</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
				<limit for="public.image" strict="YES">
					<key>min-size</key>
					<size>{280, 200}</size>
					<key>max-size</key>
					<size>{280, 200}</size>
				</limit>
			</limits>
		</binding>
		<binding type="attribute" container="none">
			<binding-key>caption</binding-key>
			<localized-name>
				<string lang="en">Caption Text</string>
			</localized-name>
			<content-types>
				<identifier>public.text</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Enter your Caption Text.</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
				<limit for="public.text" strict="YES">
					<key>max-length</key>
					<integer>200</integer>
				</limit>
			</limits>
		</binding>	
		<binding type="attribute" container="none">
			<binding-key>movie</binding-key>
			<localized-name>
				<string lang="en">Movie</string>
			</localized-name>
			<content-types>
				<identifier>public.audiovisual-content</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose a Movie.</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
				<limit for="public.audiovisual-content" strict="YES">
					<key>min-size</key>
					<size>{280, 200}</size>
					<key>max-size</key>
					<size>{280, 200}</size>
				</limit>
			</limits>
		</binding>
		<binding type="attribute" container="none">
			<binding-key>audio</binding-key>
			<localized-name>
				<string lang="en">Audio File</string>
			</localized-name>
			<content-types>
				<identifier>public.audiovisual-content</identifier>
				<identifier>pachyderm.resource</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose an audio file.</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
			</limits>
		</binding>
		<binding type="attribute" container="none">
			<binding-key>link</binding-key>
			<localized-name>
				<string lang="en">Screen Link</string>
			</localized-name>
			<content-types>
				<identifier>pachyderm.atomic.link</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Choose a screen or URL to link to.</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
			</limits>
		</binding>
		<binding type="attribute" container="none">
			 <x-auto-instantiate>YES</x-auto-instantiate>
			<binding-key>dictionary</binding-key>
			<localized-name>
				<string lang="en">Keyword Dictionary</string>
			</localized-name>
			<content-types>
				<identifier>pachyderm.general.keyword_dictionary</identifier>
				<identifier>pachyderm.abstract.menu</identifier>
			</content-types>
			<help-tag></help-tag>
			<x-help-text>Keyword Dictionary</x-help-text>
			<editable>YES</editable>
			<default-association>
				<association>
					<!-- insert serialized association here -->
				</association>
			</default-association>
			<limits>
			</limits>
		</binding>
	</binding-list> 
	<binding-validators>
	</binding-validators>
	<user-interface>
		<preferred-binding-ordering>
			<binding-key>icon</binding-key>
			<binding-key>image</binding-key>
			<binding-key>caption</binding-key>
			<binding-key>movie</binding-key>
			<binding-key>audio</binding-key>
			<binding-key>link</binding-key>
			<binding-key>dictionary</binding-key>
		</preferred-binding-ordering>
	</user-interface>
	<environment-support>
		<environment>
			<identifier>pachyderm.flash-environment</identifier>
			<min-size>{0.0, 0.0}</min-size>
			<max-size>{0.0, 0.0}</max-size>
			<auto-layout>NO</auto-layout>
			<accepts-children>NO</accepts-children>
			<resources>
				<resource></resource>
			</resources>
		</environment>
	</environment-support>
</component-definition>